{
  "name": "ifman",
  "version": "1.0.0",
  "description": "facility management company",
  "main": "app.js",
  "scripts": {
    "test": "npm run migrate && mocha server/v1/test/test.js --require @babel/register --timeout 10000 --exit || true",
    "dev": "set DEBUG=dev && nodemon --exec babel-node server/app.js",
    "migrate-test": "cross-env NODE_ENV=test npm run migrate",
    "migrate": "set DEBUG=dev && npm run drop && npm run create && npm run seed",
    "create": "babel-node server/v1/app/database/createTables.js",
    "sql-migrate": "sequelize db:migrate",
    "sql-drop": "sequelize db:migrate:undo:all",
    "seeders": "npm run seeder-users&& npm run seeder-dues&& npm run seeder-courses&& npm run seeder-events",
    "seeder-users": "sequelize db:seed --seed 20190925105015-admin-user.js && sequelize db:seed --seed 20190925105331-users.js",
    "seeder-dues": "sequelize db:seed --seed 20190925105527-dues.js && sequelize db:seed --seed 20190925110036-userDues.js",
    "seeder-courses": "sequelize db:seed --seed 20190925110541-courses.js && sequelize db:seed --seed 20190925110253-userCourses.js",
    "seeder-events": "sequelize db:seed --seed 20190925105818-events.js",
    "reset-db": "npm run drop-db&& npm run migrate&& npm run seeders",
    "drop": "babel-node server/v1/app/database/dropTables",
    "seed": "babel-node server/v1/app/database/seed.js",
    "lint": "./node_modules/.bin/eslint ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misanj/Ifman.git"
  },
  "keywords": [
    "ifma",
    "node",
    "express",
    "endpoints"
  ],
  "author": "temisan otokuefor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/misanj/Ifman/issues"
  },
  "homepage": "https://github.com/misanj/Ifman#readme",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
